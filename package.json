{
  "name": "@myuon/react-photo-edit",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/myuon/react-photo-edit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myuon/react-photo-edit.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf ./dist",
    "build": "yarn typecheck && yarn build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "postformat": "prettier --write .",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "cubic-spline": "^3.0.3",
    "image-js": "^0.35.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-css-reorder": "^0.5.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
